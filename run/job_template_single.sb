#!/bin/bash --login

#SBATCH -N 1
#SBATCH -c 1
#SBATCH --mem=16GB
#SBATCH -o ../slurm_history/slurm-%x-%A.out
#SBATCH -A cmse
#SBATCH -t 3:55:00

# NOTE: nle-dev-pyg is for dev, use nle once the experiment design is settled
conda activate nle-dev-pyg

cd $SLURM_SUBMIT_DIR
cd ..

for i in $(seq 0 4); do
    echo $@ runid=$i
    eval $@ runid=$i
done
